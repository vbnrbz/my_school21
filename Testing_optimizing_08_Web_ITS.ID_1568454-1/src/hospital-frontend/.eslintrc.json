{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prop-types": 0,
    "no-console": 1,
    "no-debugger": 2,
    "react/react-in-jsx-scope": 0,
    "react/function-component-definition": 0,
    "import/prefer-default-export": 0,
    "react/jsx-props-no-spreading": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}